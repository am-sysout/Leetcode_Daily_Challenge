class Solution {
    public String reverseVowels(String s) {
		 if(s==null || s.isEmpty())
			 return "";
		 List<Character> vList = Arrays.asList('a','e','i','o','u','A','E','I','O','U');
		 char[] ch =s.toCharArray();
		 int i=0;
		 int j=s.length()-1;
		 while(i<j) {
			 while(i<j && !vList.contains(s.charAt(i))) {
				 i++;
			 }
			 
			 while(i<j && !vList.contains(s.charAt(j))) {
				 j--;
			 }
			 if(i<=j){
			 swap(ch,i,j);
			 
             }
             i++;
             j--;
		 }
		return new String(ch);
	        
	    }
	 void swap(char[] ch , int i,int j) {
		 char t= ch[i];
		 ch[i]=ch[j];
		 ch[j]=t;
	 }
}
